	ld	de,cursor_text
	call	prttxt
	ld	a,(sys_table+106)
	ld	l,a
	ld	h,0
	call	putnum				;print & get input
	jr	z,next3
	call	getnum				;calc num in de
	ld	a,e
	ld	(sys_table+106),a
next3:
	call	crlf
;
; Main Disk (errdsk)
;
	ld	de,main1
	call	prttxt
	ld	a,(sys_table+53)
	add	a,'A'
	ld	de,fragex
	ld	(de),a
	call	input
	jr	z,next4
	ld	a,(hl)
	and	5fh
	cp	'A'
	jr	c,next4
	cp	'D'+1
	jr	nc,next4
	sub	'A'
	ld	(sys_table+53),a
next4:
;
; Wboot Disk (rstdsk)
;
;	ld	de,main2
