;next5:
	call	crlf
;
; Time for 1st repeat
;
	ld	de,reptx1
	call	prttxt
	ld	hl,(sys_table+32)
	call	putnum
	jr	z,next6				;jp if no input
	call	getnum
	ld	(sys_table+32),de
next6:
;
; Time till next repeat
