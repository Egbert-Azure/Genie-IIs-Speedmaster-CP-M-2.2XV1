	ld	de,iobyte_txt
	call	prttxt
	ld	a,(sys_table+0)
	ld	l,a
	ld	h,0
	call	putnum				;print & get input
	jr	z,next2
	call	getnum				;calc num in de
	ld	a,e
	ld	(sys_table+0),a
next2:
;
; Ask cursor
